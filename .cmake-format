parse:
  additional_commands:
    target_include_directories:
      pargs: 1
      flags:
        - SYSTEM
        - BEFORE
      kwargs:
        INTERFACE: '*'
        PUBLIC: '*'
        PRIVATE: '*'
markup:
  bullet_char: '*'
  enum_char: .
  first_comment_is_literal: true
misc:
  per_command: {}

